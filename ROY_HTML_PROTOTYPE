    @keyframes spin {
      from {
        transform: rotate(0deg);
      }
      to {
        transform: rotate(360deg);
      }
    }

    /* Apply the animation to the image */
    .animated-image {
      width: 300px;        /* Adjust the size as needed. */
      height: 300px;
      animation: spin 5s linear infinite; /* 5s duration, linear timing, infinite repeat */
    }
  </style>
